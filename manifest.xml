<?xml version="1.0" encoding="utf-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="TaskPaneApp">
  <Id>888eae7f-c8e8-4f09-823a-a3d0664cc7b3</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>ahmetozel excel</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Highlight Selected Cells"/>
  <Description DefaultValue="Highlight selected cells in yellow."/>
  <IconUrl DefaultValue="https://ahmetozel.github.io/excel-extension/icon.png"/>
  <HighResolutionIconUrl DefaultValue="https://ahmetozel.github.io/excel-extension/icon@2x.png"/>
  <SupportUrl DefaultValue="https://ahmetozel.github.io/excel-extension/iletisim.html"/>
  
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  
  <Requirements>
    <Sets>
      <Set Name="ExcelApi" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  
  <FormSettings>
    <Form xsi:type="TaskPane">
      <SourceLocation DefaultValue="https://ahmetozel.github.io/excel-extension/index.html"/>
      <RequestedHeight>300</RequestedHeight>
    </Form>
  </FormSettings>
</OfficeApp>